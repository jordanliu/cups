{
    "source": {
        "include": ["routes"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs|client|nginx|uploads)"
    },
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "recurse": true,
        "destination": "./docs/"
    }
}
